<!-- index.ejs -->
<html>
  <head>
    <title>Homepage</title>
    <link rel="stylesheet" href="/styles.css">
  </head>
  <body>
    
    <ul>
      <% messages.forEach(message => { %>
        <li>
          Message: <%= message.text %><br>
          User: <%= message.user %><br>
          Date: <%= message.date %>
        </li>
      <% }) %>
    </ul>

  </body>
</html>

